<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".PartyrunActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:orientation="vertical">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="91dp"
                android:layout_height="46dp"
                android:layout_marginLeft="20dp"
                app:srcCompat="@drawable/logo" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/party_use_background"
        android:orientation="vertical"
        android:paddingLeft="25dp"
        android:paddingRight="25dp">

        <LinearLayout
            android:layout_width="362dp"
            android:layout_height="28dp"
            android:orientation="vertical"></LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:layout_marginBottom="20dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />

                <ImageButton
                    android:id="@+id/imgb_run"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:background="#00000000"
                    app:srcCompat="@drawable/x" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_run_partypage_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="15sp"
                android:fontFamily="@font/notosansregular"
                android:textColor="@color/black"
                android:includeFontPadding="false"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_run_partypage_name"
                    android:layout_width="245dp"
                    android:layout_height="55dp"
                    android:layout_weight="1"
                    android:text="TextView"
                    android:textSize="22sp"
                    android:fontFamily="@font/notosansbold"
                    android:textColor="@color/black"
                    android:includeFontPadding="false"
                    />

                <TextView
                    android:id="@+id/tv_run_partypage_peo"
                    android:layout_width="91dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="TextView" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_runp1"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:background="#FFFFFF"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="13dp"
                    android:gravity="center">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:src="@drawable/profile_imgtwo"
                        app:civ_border_color="#B1B1B1"
                        app:civ_border_width="1dp" />

                </FrameLayout>


                <TextView
                    android:id="@+id/tv_makepartyLast"
                    android:layout_width="231dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="13dp"
                    android:fontFamily="@font/notosansregular"
                    android:includeFontPadding="false"
                    android:text="김요환 (남)"
                    android:textColor="@color/black" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_runp2"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:background="#FFFFFF"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="13dp"
                    android:gravity="center">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:src="@drawable/profile_imgtwo"
                        app:civ_border_color="#B1B1B1"
                        app:civ_border_width="1dp" />

                </FrameLayout>


                <TextView
                    android:id="@+id/tv_Myname"
                    android:layout_width="231dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="13dp"
                    android:fontFamily="@font/notosansregular"
                    android:includeFontPadding="false"
                    android:text="김요환 (남)"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_runp3"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:background="#FFFFFF"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="13dp"
                    android:gravity="center">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:src="@drawable/profile_imgtwo"
                        app:civ_border_color="#B1B1B1"
                        app:civ_border_width="1dp" />

                </FrameLayout>


                <TextView
                    android:id="@+id/tv_MyName2"
                    android:layout_width="231dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="13dp"
                    android:fontFamily="@font/notosansregular"
                    android:includeFontPadding="false"
                    android:text="김요환 (남)"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_runp4"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:background="#FFFFFF"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="13dp"
                    android:gravity="center">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:src="@drawable/profile_imgtwo"
                        app:civ_border_color="#B1B1B1"
                        app:civ_border_width="1dp" />

                </FrameLayout>


                <TextView
                    android:id="@+id/tv_MyName3"
                    android:layout_width="231dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="13dp"
                    android:fontFamily="@font/notosansregular"
                    android:includeFontPadding="false"
                    android:text="김요환 (남)"
                    android:textColor="@color/black" />
            </LinearLayout>


            <!--            <LinearLayout-->
            <!--                android:id="@+id/ll_runp4"-->
            <!--                android:layout_width="match_parent"-->
            <!--                android:layout_height="wrap_content"-->
            <!--                android:background="@drawable/in_party_people_background"-->
            <!--                android:orientation="horizontal">-->

            <!--                <ImageView-->
            <!--                    android:id="@+id/imageView9"-->
            <!--                    android:layout_width="71dp"-->
            <!--                    android:layout_height="31dp"-->
            <!--                    app:srcCompat="@drawable/a" />-->

            <!--                <TextView-->
            <!--                    android:id="@+id/tv_MyName3"-->
            <!--                    android:layout_width="wrap_content"-->
            <!--                    android:layout_height="31dp"-->
            <!--                    android:layout_weight="1"-->
            <!--                    android:text="김요환 (남)" />-->
            <!--            </LinearLayout>-->

        </LinearLayout>

        <TextView
            android:id="@+id/tv_run_partypage_space"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:text="위치 :"
            android:fontFamily="@font/notosansregular"
            android:textColor="@color/black"
            android:includeFontPadding="false"
            />

        <TextView
            android:id="@+id/tv_run_partypage_index"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:text="누구보다 빠르게 밥만 먹고\n도망갑니다 알아서 참여하세요!"
            android:textSize="18sp"
            android:fontFamily="@font/notosansregular"
            android:textColor="@color/black"
            android:includeFontPadding="false"
            />

        <Button
            android:id="@+id/btn_runrunrun"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_main_menu"
            android:text="도망가기"
            android:textColor="@color/white"
            android:fontFamily="@font/notosansbold"
            android:includeFontPadding="false"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            />
    </LinearLayout>

</LinearLayout>